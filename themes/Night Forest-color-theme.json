	"tokenColors": [
		{
			"name": "Types, Classes",
			"scope": [
				"entity.name.type",
				"entity.other.inherited-class",
				"keyword.other.type",
				"keyword.type",
				"punctuation.definition.annotation",
				"storage.modifier.import",
				"storage.modifier.package",
				"storage.type.annotation",
				"storage.type.built-in",
				"storage.type.generic",
				"storage.type.java",
				"storage.type.groovy",
				"storage.type.primitive",
				"storage.type.concrete",
				"support.class",
				"support.other.namespace",
				"support.type",
				// "variable.language.this",

				// # Lang. specific overrides
				"meta.use.rust",
				"meta.import.rust",
				"entity.name.module.rust",
				// "entity.name.namespace.rust",
				// "entity.name.namespace.cpp",
				"entity.name.scope-resolution.cpp",
				"entity.name.namespace",
			],
			"settings": {
				"foreground": "#FFF890"
			}
		},
		{
			"name": "Keys, Tags, Properties, Templates",
			"scope": [
				"constant.other.character-class",
				"entity.name.tag",
				"heading",
				"meta.object-literal.key",
				"punctuation.definition.list.begin.markdown",
				"punctuation.definition.list.end.markdown",
				"punctuation.definition.template-expression",
				"punctuation.definition.interpolation",
				"punctuation.section.embedded",
				"support.type.property-name",
				"variable.object.property",
				"variable.other.enummember",
				"string.interpolated.expression",

				// # Lang overrides
				"entity.name.section.group-title.ini",
				"keyword.other.definition.ini",
			],
			"settings": {
				"foreground": "#FF6675"
			}
		},
		{
			"name": "Escape chars, regex, links",
			"scope": [
				"constant.character.escape",
				"markup.underline.link",
				"string.regexp",
				"string.url",
			],
			"settings": {
				"foreground": "#e0af27"
			}
		},
		{
			"name": "Functions, Descriptors",
			"scope": [
				"entity.name.function",
				"entity.other.attribute-name.id.css",
				"meta.function-call.generic",
				"string.other.link",
				"support.function",
				"variable.language.super",

				// # U know the deal by now
				"entity.name.scope-resolution.function.call", // C++
			],
			"settings": {
				"foreground": "#3EC3FF"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"meta.brace",
				"meta.interpolation",
				"meta.embedded.line",
				"support.type.property-name.css",
			],
			"settings": {
				"foreground": "#EEEEEE"
			}
		},
		{
			"name": "Variable paremeters",
			"scope": [
				"variable.parameter",
				"entity.other.attribute-name",
				"keyword.operator.quantifier.regexp",

				// # Quickfixes for symbols getting operator colors
				// "entity.name.variable.parameter.cs",
			],
			"settings": {
				"foreground": "#EEEEEE"
			}
		},
		{
			"name": "Punctuation: Accessors, seperators, terminators",
			"scope": [
				// "punctuation.accessor",
				// "punctuation.definition.block",


				// "punctuation.dot",
				// "punctuation.separator",

				// "punctuation.terminator",

				// Lang overrides
				"punctuation.definition.entity.ini",
				"punctuation.separator.key-value.ini",
				"punctuation.separator.java",
				"punctuation.semi.rust",
				"support.function.library.lua",
				// C/C++
				// "punctuation.definition.directive",

				// # Quickfixes for symbols getting operator colors
				// "keyword.operator.type.annotation.ts",
				// "keyword.operator.namespace.rust",
				// "keyword.operator.key-value.rust",
				// "keyword.operator.access.dot.rust",
			],
			"settings": {
				"foreground": "#EEEEEE"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"markup.inline",
				"markup.quote",
				"string.other.link.description",
				"string",

				// # Lang overrides
				//"source.ini"
			],
			"settings": {
				"foreground": "#2ebb74"
			}
		},
		{
			"name": "Comments",
			"scope": [
				"comment"
			],
			"settings": {
				"foreground": "#00AAA5"
			}
		},
		{
			"name": "Operators",
			"scope": [
				"keyword.operator",

				// # Lang overrides
				//"punctuation.separator.scope-resolution", //C++

			],
			"settings": {
				"foreground": "#27E0D7"
			}
		},
		{
			"name": "Keywords, Operators",
			"scope": [
				"keyword.operator.new",
				"keyword",
				"markup.italic",
				"punctuation.definition.block.tag",
				"storage",
				"storage.modifier",
				"storage.type",

				// Lang overrides

			],
			"settings": {
				"foreground": "#cba0ff"
			}
		},
		{
			"name": "Numbers, Constants",
			"scope": [
				"markup.bold",
				"constant",
				"support.constant",
				// "variable.other.constant",
				"variable.language",

			],
			"settings": {
				"foreground": "#E54286"
			}
		},
		{
			"name": "Quotes",
			"scope": [
				"markup.quote",
				"markup.italic"
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Headings",
			"scope": [
				"heading",
				"markup.bold"
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "token.info-token",
			"settings": {
				"foreground": "#3E83D1"
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#FFF890"
			}
		},
		{
			"scope": "token.error-token",
			"settings": {
				"foreground": "#F04B3C"
			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
				"foreground": "#cba0ff"
			}
		}
	],
	"semanticHighlighting": false,
	"semanticTokenColors": {}
}
